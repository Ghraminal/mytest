function check(){
	if(document.frm.car_number.value == ""){
		alert("차 번호가 입력되지 않았습니다.")
		document.frm.car_number.focus();
	}
	else if(document.frm.car_number.value == ""){
		alert("차 번호가 입력되지 않았습니다.")
		document.frm.car_number.focus();
	}
	else if(document.frm.car_number.value == ""){
		alert("차 번호가 입력되지 않았습니다.")
		document.frm.car_number.focus();
	}
	else if(document.frm.car_number.value == ""){
		alert("차 번호가 입력되지 않았습니다.")
		document.frm.car_number.focus();
	}
	else if(document.frm.car_number.value == ""){
		alert("차 번호가 입력되지 않았습니다.")
		document.frm.car_number.focus();
	}
}