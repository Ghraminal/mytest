function check(){
	if(document.frm.car_number.value == ""){
		alert("차 번호가 입력되지 않았습니다.");
		document.frm.car_number.focus();
	}
	else if(document.frm.owner_name.value == ""){
		alert("소유자 이름이 입력되지 않았습니다.");
		document.frm.owner_name.focus();
	}
	else if(document.frm.location.value == ""){
		alert("주차 위치가 입력되지 않았습니다.");
		document.frm.location.focus();
	}
	else if(document.frm.entrance_time.value == ""){
		alert("입차 시간이 입력되지 않았습니다.");
		document.frm.entrance_time.focus();
	}
	else{
		document.frm.submit();
	}
}
function resett(){
	alert("입력 내용을 지우고, 처음부터 다시 작성합니다.");
	document.frm.reset();
}
function check(){
	if(document.frm.car_number.value == ""){
		alert("차 번호가 입력되지 않았습니다.");
		document.frm.car_number.focus();
	}
	else if(document.frm.owner_name.value == ""){
		alert("소유자 이름이 입력되지 않았습니다.");
		document.frm.owner_name.focus();
	}
	else if(document.frm.location.value == ""){
		alert("주차 위치가 입력되지 않았습니다.");
		document.frm.location.focus();
	}
	else if(document.frm.entrance_time.value == ""){
		alert("입차 시간이 입력되지 않았습니다.");
		document.frm.entrance_time.focus();
	}
	else{
		document.frm.submit();
	}
}